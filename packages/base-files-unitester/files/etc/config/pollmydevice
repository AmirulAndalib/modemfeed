config interface '0'
        option mode 'server'
        option desc 'AB'
        option devicename '/dev/com1'
        option baudrate '115200'
        option bytesize '8'
        option parity 'none'
        option flowcontrol 'none'
        option stopbits '1'
        option server_port '33000'
        option holdconntime '30'
        option modbus_gateway '0'
        option quiet '0'

config interface '1'
        option mode 'server'
        option desc 'console'
        option devicename '/dev/com0'
        option baudrate '115200'
        option bytesize '8'
        option parity 'none'
        option flowcontrol 'none'
        option stopbits '1'
        option server_port '33001'
        option holdconntime '30'
        option modbus_gateway '0'
        option quiet '0'
